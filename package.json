{
  "name": "TheDarkRoom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "buildJavascriptBundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "buildAndroidApk": "npm run buildJavascriptBundle && cd android && ./gradlew assembleRelease",
    "buildAndroidApkDebug": "npm run buildJavascriptBundle && cd android && ./gradlew assembleDebug",
    "buildAndroidBundle": "npm run buildJavascriptBundle && cd android && ./gradlew bundleRelease",
    "svgTransform": "npx @svgr/cli --native -d src/components/icons src/assets/svg"
  },
  "dependencies": {
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/cameraroll": "^4.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^8.3.0",
    "@react-native-firebase/messaging": "^7.6.1",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "deprecated-react-native-listview": "0.0.6",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-appearance": "^0.3.4",
    "react-native-bundle-splitter": "^1.0.9",
    "react-native-fast-storage": "^0.1.4",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-hr-component": "^2.0.3",
    "react-native-icon-badge": "^1.1.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-scalable-image": "^1.0.0",
    "react-native-screens": "^2.9.0",
    "react-native-share": "^3.7.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-toggle-element": "^1.0.0",
    "react-redux": "^7.2.1",
    "reanimated-bottom-sheet": "^1.0.0-alpha.20",
    "redux": "^4.0.5",
    "redux-act": "^1.8.0",
    "redux-persist": "^6.0.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/react-native": "^0.63.2",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-rename": "^2.4.1",
    "react-test-renderer": "16.13.1",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
